vector<int> Solution::wave(vector<int> &a) {
    sort(a.begin(),a.end());
    if(a.size()%2==0)
    {
    for(int i=0;i<a.size();i+=2)
    {
        int temp=a[i];
        a[i]=a[i+1];
        a[i+1]=temp;
    }
    }
    else
    for(int i=0;i<a.size()-1;i+=2)
    {
       int temp=a[i];
        a[i]=a[i+1];
        a[i+1]=temp; 
    }
    return a;
}
